<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/layout/custom_game/gg/gg.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/layout/custom_game/gg/gg.js" /> 
	</scripts>
	
	<Panel class="MainGG">
		<Panel id="Root" class="Root">
			<Panel id="Title">
				<Label class="UILabelLarge" html="true" text="#gg_panel_description" />
			</Panel>
			<Panel id="Timer">
				<Label class="GGTimer" id="GGTimer" text="4" />
			</Panel>
			<Button id="MainButton" onmouseactivate="CancelGG();">
				<Label class="GG_button_label" text="#gg_cancel"/>
			</Button>		
		</Panel>
	</Panel>
</root>
