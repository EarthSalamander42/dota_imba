<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/styles/custom_game/shared_scoreboard_styles.css" />
		<include src="file://{resources}/styles/custom_game/multiteam_flyout_scoreboard.css" />
		<include src="file://{resources}/layout/custom_game/party_highlighter/party_highlighter.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/layout/custom_game/party_highlighter/party_highlighter.js" />
